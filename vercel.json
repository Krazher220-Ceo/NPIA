{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/apply.html",
      "dest": "/apply.html"
    },
    {
      "src": "/index_landing.html",
      "dest": "/index_landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

